package com.sicap.clientes.vo.cartera;

import com.sicap.clientes.vo.SaldoIBSVO;
import java.io.Serializable;
import java.sql.Date;

/**
 *
 * @author avillanueva
 */
public class DevengamientoVO implements Serializable{
    
    public int idDevengado;
    public int numPago;
    public Date fechaDevengamiento;
    public int numCliente;
    public int numCredito;
    public double interes;
    public double ivaInteres;
    public int devengado;

    public DevengamientoVO() {
    }

    public DevengamientoVO(double interes, double ivaInteres) {
        this.interes = interes;
        this.ivaInteres = ivaInteres;
    }

    public DevengamientoVO(int numPago, Date fechaDevengamiento, int numCliente, int numCredito, double interes, double ivaInteres, int devengado) {
        this.numPago = numPago;
        this.fechaDevengamiento = fechaDevengamiento;
        this.numCliente = numCliente;
        this.numCredito = numCredito;
        this.interes = interes;
        this.ivaInteres = ivaInteres;
        this.devengado = devengado;
    }

    public DevengamientoVO(int idDevengado, int numPago, Date fechaDevengamiento, int numCliente, int numCredito, double interes, double ivaInteres) {
        this.idDevengado = idDevengado;
        this.numPago = numPago;
        this.fechaDevengamiento = fechaDevengamiento;
        this.numCliente = numCliente;
        this.numCredito = numCredito;
        this.interes = interes;
        this.ivaInteres = ivaInteres;
    }
    
    public int getNumPago() {
        return numPago;
    }

    public void setNumPago(int numPago) {
        this.numPago = numPago;
    }

    public Date getFechaDevengamiento() {
        return fechaDevengamiento;
    }

    public void setFechaDevengamiento(Date fechaDevengamiento) {
        this.fechaDevengamiento = fechaDevengamiento;
    }

    public int getNumCliente() {
        return numCliente;
    }

    public void setNumCliente(int numCliente) {
        this.numCliente = numCliente;
    }

    public int getNumCredito() {
        return numCredito;
    }

    public void setNumCredito(int numCredito) {
        this.numCredito = numCredito;
    }

    public double getInteres() {
        return interes;
    }

    public void setInteres(double interes) {
        this.interes = interes;
    }

    public double getIvaInteres() {
        return ivaInteres;
    }

    public void setIvaInteres(double ivaInteres) {
        this.ivaInteres = ivaInteres;
    }

    public int getDevengado() {
        return devengado;
    }

    public void setDevengado(int devengado) {
        this.devengado = devengado;
    }

    public int getIdDevengado() {
        return idDevengado;
    }

    public void setIdDevengado(int idDevengado) {
        this.idDevengado = idDevengado;
    }
    
}
