package com.sicap.clientes.vo.inffinix;

import java.io.Serializable;

public class CodigoAccionVO implements Serializable{
	
	public String codigoAccion;
	public String descripcion;
	
	public CodigoAccionVO(){
		codigoAccion = null;
		descripcion = null;
	}
	
	public String toString(){
		String resultado;
		
		resultado = "codigoAccion=[" + codigoAccion + "],";
		resultado += "descripcion=[" + descripcion + "]";
		return resultado;
	}

}
