package com.sicap.clientes.vo.cartera;

import java.io.Serializable;
import java.sql.Timestamp;

public class PagosConcentradoraVO implements Serializable{
	
	public int numCte;
	public Timestamp fecha;
	public String referencia;
	public double retiro;
	public double deposito;
	public double saldo;
	public String stsRef;
	public int banco;
	public String fechaRep;
	public int idCte;
	public boolean pm;
	public boolean pr;
	public String descripcionBanco;
	
	public PagosConcentradoraVO(){
		numCte = 0;
		fecha = null;
		referencia = null;
		retiro = 0;
		deposito = 0;
		saldo = 0;
		stsRef = null;
		banco = 0;
		fechaRep = null;
		idCte = 0;
		pm = false;
		pr = false;
		descripcionBanco = null;
	}
	
	public String toString(){
		String respuesta = null;
		respuesta = "numCte=[" + numCte + "],";
		respuesta += "fecha=[" + fecha + "],";
		respuesta += "referencia=[" + referencia + "],";
		respuesta += "retiro=[" + retiro + "],";
		respuesta += "deposito=[" + deposito + "],";
		respuesta += "saldo=[" + saldo + "],";
		respuesta += "stsRef=[" + stsRef + "],";
		respuesta += "banco=[" + banco + "],";
		respuesta += "fechaRep=[" + fechaRep + "],";
		respuesta += "idCte=[" + idCte + "],";
		respuesta += "pm=[" + pm + "],";
		respuesta += "pr=[" + pr + "],";
		respuesta += "descripcionBanco=[" + descripcionBanco + "]";
		
		return respuesta;
	}
}
