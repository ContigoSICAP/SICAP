package com.sicap.clientes.vo;

import java.io.Serializable;
import java.sql.Date;

public class DocumentoFormalizVO implements Serializable{

	public String tipoDocumento;
	public int tipoOperacion;
	public int numSucursal;
	public int numConvenio;
	public String	rutaArchivo;
	public String	plantilla;
	public int		variable;

	public DocumentoFormalizVO(){

		tipoDocumento = null;
		tipoOperacion = 0;
		numSucursal = 0;
		numConvenio = 0;
		rutaArchivo = null;
		plantilla 	= null;
		variable	= 0;
	}


	public String toString(){
		String respuesta = null;
		respuesta = "tipoDocumento=["+tipoDocumento+"],";
		respuesta += "tipoOperacion=["+tipoOperacion+"],";
		respuesta += "numSucursal=["+numSucursal+"],";
		respuesta += "numConvenio	=["+numConvenio+"],";
		respuesta += "rutaArchivo=["+rutaArchivo+"],";
		respuesta += "plantilla=["+plantilla+"],";
		respuesta += "variable=["+variable+"],";
		return respuesta;
	}

}
