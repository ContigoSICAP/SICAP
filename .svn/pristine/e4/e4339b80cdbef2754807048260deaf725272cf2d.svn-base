
package com.sicap.clientes.vo;

import java.util.Date;

/**
 *
 * @author nmejia
 */
public class AmortizacionPagareVO 
{
    private int idAmort;
    private PagareVO pagaresArray[];
    private int idPagare;
    private int numPago;
    private Date fechaPago;
    private double capital;
    private String interes;
    private double iva;
    private int estatus;
    private PagareVO pagares;
    private String nombrePagare;
    private String nombreLineaCredito;

    public AmortizacionPagareVO(int idPagare, int numPago, Date fechaPago, double capital, String interes, double iva) {
        this.idPagare = idPagare;
        this.numPago = numPago;
        this.fechaPago = fechaPago;
        this.capital = capital;
        this.interes = interes;
        this.iva = iva;
    }
    

    public AmortizacionPagareVO() {
    }

    public AmortizacionPagareVO(int idAmort, String nombrePagare) 
    {
        this.idAmort = idAmort;
        this.nombrePagare = nombrePagare;
    }
    
    

    public int getIdAmort() {
        return idAmort;
    }

    public void setIdAmort(int idAmort) {
        this.idAmort = idAmort;
    }

    public int getIdPagare() {
        return idPagare;
    }

    public void setIdPagare(int idPagare) {
        this.idPagare = idPagare;
    }

    public int getNumPago() {
        return numPago;
    }

    public void setNumPago(int numPago) {
        this.numPago = numPago;
    }

    public Date getFechaPago() {
        return fechaPago;
    }

    public void setFechaPago(Date fechaPago) {
        this.fechaPago = fechaPago;
    }

    public double getCapital() {
        return capital;
    }

    public void setCapital(double capital) {
        this.capital = capital;
    }

    public String getInteres() {
        return interes;
    }

    public void setInteres(String interes) {
        this.interes = interes;
    }

    public double getIva() {
        return iva;
    }

    public void setIva(double iva) {
        this.iva = iva;
    }

    public int getEstatus() {
        return estatus;
    }

    public void setEstatus(int estatus) {
        this.estatus = estatus;
    }

    public PagareVO[] getPagaresArray() {
        return pagaresArray;
    }

    public void setPagares(PagareVO[] pagaresArray) {
        this.pagaresArray = pagaresArray;
    }

    public PagareVO getPagares() {
        return pagares;
    }

    public void setPagares(PagareVO pagares) {
        this.pagares = pagares;
    }

    public String getNombrePagare() {
        return nombrePagare;
    }

    public void setNombrePagare(String nombrePagare) {
        this.nombrePagare = nombrePagare;
    }
    
        public void setNombreLineaCredito(String nombreLineaCredito) {
        this.nombreLineaCredito = nombreLineaCredito;
    }

    public String getNombreLineaCredito() {
        return nombreLineaCredito;
    }
}
