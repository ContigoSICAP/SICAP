package com.sicap.clientes.vo;

import java.io.Serializable;

public class ClienteMorosoVO implements Serializable{

	public String referencia;
	public int numOperacion;
	public String rfc;
	public int numDiasVencidos;

	public ClienteMorosoVO(){

		referencia = null;
		numOperacion = 0;
		rfc = null;
		numDiasVencidos = 0;
	}


	public String toString(){
		String respuesta = null;
		respuesta = "referencia=["+referencia+"],";
		respuesta = "numOperacion=["+numOperacion+"],";
		respuesta += "rfc=["+rfc+"],";
		respuesta += "numDiasVencidos=["+numDiasVencidos+"],";
		return respuesta;
	}

}
