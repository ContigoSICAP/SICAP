package com.sicap.clientes.vo;

import java.io.Serializable;
import java.util.Date;

public class PlaneacionRenovacionVO implements Serializable{

	public int numEjecutivo;
	public int numSucursal;
	public int numGrupo;
	public int numCiclo;
	public int integrantes;
	public int integrantesTotal;
	public Date fechaVencimiento;
	public int renueva;
	public int numMotivo;

	public PlaneacionRenovacionVO(){

		numEjecutivo = 0;
		numSucursal = 0;
		numGrupo = 0;
		numCiclo = 0;
		integrantes = 0;
		integrantesTotal = 0;
		fechaVencimiento = null;
		renueva = 0;
		numMotivo = 0;
	}


	public String toString(){
		String respuesta = null;
		respuesta = "numEjecutivo=["+numEjecutivo+"],";
		respuesta = "numSucursal=["+numSucursal+"],";
		respuesta += "numGrupo=["+numGrupo+"],";
		respuesta += "numCiclo=["+numCiclo+"],";
		respuesta += "integrantes =["+integrantes+"],";
		respuesta += "integrantesTotal =["+integrantesTotal+"],";
		respuesta += "fechaVencimiento=["+fechaVencimiento+"],";
		respuesta += "renueva=["+renueva+"],";
		respuesta += "numMotivo=["+numMotivo+"]";
		return respuesta;
	}

}
