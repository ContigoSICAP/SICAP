package com.sicap.clientes.vo.cartera;

import java.io.Serializable;
import java.sql.Date;

public class CondonacionesVO  implements Serializable{
	
	public int numCliente;
	public int numCredito;
	public int numDividendo;
	public double monto;
	public String rubro;
	public String comentario;
	public Date fecha;
	public String usuario;
	
	public String toString(){
		String respuesta = null;
		respuesta += "numCliente=[" + numCliente + "],";
		respuesta += "numCredito=[" + numCredito + "],";
		respuesta += "numDividendo=[" + numDividendo + "],";
		respuesta += "rubro=[" + rubro + "],";
		respuesta += "monto=[" + monto + "],";
		respuesta += "comentario=[" + comentario + "],";
		respuesta += "usuario=[" + usuario + "],";
		return respuesta;
	}
	

}
