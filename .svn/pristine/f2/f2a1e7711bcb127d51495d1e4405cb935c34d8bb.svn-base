package com.sicap.clientes.vo;

import java.io.Serializable;
import java.sql.Date;


public class PagoExcedenteVO implements Serializable{

        public Date fechaMovimiento;
        public String referencia;
        public double montoExcedente;
        public int numCliente;
        public String numCuentaContable;
        public int bancoReferencia;
        public int idContabilidad;

		
		public PagoExcedenteVO() {
					referencia="";
					montoExcedente=0.0;
					numCliente=0;
					numCuentaContable="";
					bancoReferencia=0;
					idContabilidad=0;
					
		}


		public String toString(){
			String respuesta = null;
			respuesta = "fechaMovimiento=["+fechaMovimiento+"],";
			respuesta += "referencia=["+referencia+"],";
			respuesta += "montoExcedente=["+montoExcedente+"],";
			respuesta += "numCliente=["+numCliente+"],";
			respuesta += "numCuentaContable=["+numCuentaContable+"]";
			respuesta += "bancoReferencia=["+bancoReferencia+"]";
			respuesta += "idContabilidad=["+idContabilidad+"]";
			return respuesta;
		}



	
	
}
