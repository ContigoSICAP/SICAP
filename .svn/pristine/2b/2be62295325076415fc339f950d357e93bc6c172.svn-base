package com.sicap.clientes.vo;

import java.io.Serializable;
import java.sql.Timestamp;
import java.util.ArrayList;
import java.util.List;

/**
 *
 * @author avillanueva
 */
public class AuditoresVO implements Serializable{
    
    public int numAuditor;
    public String nombre;
    public String apPaterno;
    public String apMaterno;
    public String rfc;
    public int estatus;
    public Timestamp fechaAlta;
    public Timestamp fechaBaja;
    public int idSucursal;
    public ArrayList arrSucursal;

    public AuditoresVO() {
    }

    public AuditoresVO(int numAuditor, String nombre, String apPaterno, String apMaterno, String rfc) {
        this.numAuditor = numAuditor;
        this.nombre = nombre;
        this.apPaterno = apPaterno;
        this.apMaterno = apMaterno;
        this.rfc = rfc;
    }

    public AuditoresVO(int numAuditor, String nombre, String apPaterno, String apMaterno, String rfc, int estatus) {
        this.numAuditor = numAuditor;
        this.nombre = nombre;
        this.apPaterno = apPaterno;
        this.apMaterno = apMaterno;
        this.rfc = rfc;
        this.estatus = estatus;
    }

    public AuditoresVO(int numAuditor, String nombre, String apPaterno, String apMaterno, String rfc, int estatus, Timestamp fechaAlta) {
        this.numAuditor = numAuditor;
        this.nombre = nombre;
        this.apPaterno = apPaterno;
        this.apMaterno = apMaterno;
        this.rfc = rfc;
        this.estatus = estatus;
        this.fechaAlta = fechaAlta;
    }

    public AuditoresVO(int numAuditor, String nombre, String apPaterno, String apMaterno, String rfc, int estatus, Timestamp fechaAlta, Timestamp fechaBaja) {
        this.numAuditor = numAuditor;
        this.nombre = nombre;
        this.apPaterno = apPaterno;
        this.apMaterno = apMaterno;
        this.rfc = rfc;
        this.estatus = estatus;
        this.fechaAlta = fechaAlta;
        this.fechaBaja = fechaBaja;
    }

    public AuditoresVO(int idSucursal) {
        this.idSucursal = idSucursal;
    }

    public int getNumAuditor() {
        return numAuditor;
    }

    public void setNumAuditor(int numAuditor) {
        this.numAuditor = numAuditor;
    }

    public String getNombre() {
        return nombre;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public String getApPaterno() {
        return apPaterno;
    }

    public void setApPaterno(String apPaterno) {
        this.apPaterno = apPaterno;
    }

    public String getApMaterno() {
        return apMaterno;
    }

    public void setApMaterno(String apMaterno) {
        this.apMaterno = apMaterno;
    }

    public String getRfc() {
        return rfc;
    }

    public void setRfc(String rfc) {
        this.rfc = rfc;
    }

    public int getEstatus() {
        return estatus;
    }

    public void setEstatus(int estatus) {
        this.estatus = estatus;
    }

    public Timestamp getFechaAlta() {
        return fechaAlta;
    }

    public void setFechaAlta(Timestamp fechaAlta) {
        this.fechaAlta = fechaAlta;
    }

    public Timestamp getFechaBaja() {
        return fechaBaja;
    }

    public void setFechaBaja(Timestamp fechaBaja) {
        this.fechaBaja = fechaBaja;
    }

    public int getIdSucursal() {
        return idSucursal;
    }

    public void setIdSucursal(int idSucursal) {
        this.idSucursal = idSucursal;
    }

    public ArrayList getArrSucursal() {
        return arrSucursal;
    }

    public void setArrSucursal(ArrayList arrSucursal) {
        this.arrSucursal = arrSucursal;
    }
}
