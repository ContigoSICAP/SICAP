package com.sicap.clientes.vo;

import java.io.Serializable;
import java.sql.Timestamp;

public class DireccionGenericaVO implements Serializable{

	public int idDireccion;
	public int idEstado;
	public String estado;
	public int idMunicipio;
	public String municipio;
	public String ciudad;
	public int idColonia;
	public String colonia;
	public String calle;
	public String numeroExterior;
	public String numeroInterior;
	public String cp;
	public String asentamiento_cp;
	public Timestamp fechaCaptura;

	public DireccionGenericaVO(){

		idDireccion = 0;
		idEstado = 0;
		estado = null;
		municipio = null;
		ciudad = null;
		idColonia = 0;
		colonia = null;
		calle = null;
		numeroExterior = null;
		numeroInterior = null;
		cp = null;
		asentamiento_cp = null;
		fechaCaptura = null;

	}
	

	public String toString(){
		String respuesta = null;
		respuesta = "idDireccion=["+idDireccion+"],";
		respuesta += "idEstado=["+idEstado+"],";
		respuesta += "estado=["+estado+"],";
		respuesta += "idMunicipio=["+idMunicipio+"],";
		respuesta += "municipio=["+municipio+"],";
		respuesta += "ciudad=["+ciudad+"],";
		respuesta += "idColonia=["+idColonia+"],";
		respuesta += "colonia=["+colonia+"],";
		respuesta += "calle=["+calle+"],";
		respuesta += "numeroExterior=["+numeroExterior+"],";
		respuesta += "numeroInterior=["+numeroInterior+"],";
		respuesta += "cp=["+cp+"],";
		respuesta += "asentamiento_cp=["+asentamiento_cp+"],";
		respuesta += "fechaCaptura=["+fechaCaptura+"]";
		return respuesta;
	}


}
